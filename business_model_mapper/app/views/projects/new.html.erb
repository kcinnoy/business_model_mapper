
<%= form_with(model: @project, local: true) do |p| %>
    
    <%= p.label :name %><br />
    <%= p.text_field :name %><br /><br />

    <%= p.fields_for :resources do |r| %>
        <%= r.label :name %><br />
        <%= r.text_field :name %><br />
        <%= r.label :resource_type %><br />
        <%= r.text_field :resource_type %><br />
        <%= r.label :Qty %><br />
        <%= r.number_field :qty %><br />
    <% end %>

    <%= p.fields_for @strategic_goal do |r| %>
        <%= r.label :id %><br />
        <%= r.text_field :id %><br />
    <% end %>


        
 <%= p.submit 'Create Project' %>
<% end %>